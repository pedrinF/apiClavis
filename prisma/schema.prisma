generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Acesso {
  id_acesso      Int      @id @default(autoincrement())
  data_acesso    DateTime
  hora_acesso    String
  pessoas_acesso String
  local_acesso   String
}

model Usuario {
  id_usuario    Int    @id @default(autoincrement())
  nome_usuario  String
  senha_usuario String
}

model Pessoa {
  id_pessoa   Int    @id @default(autoincrement())
  nome_pessoa String
  cpf_pessoa  String
  rg_pessoa   String
}
